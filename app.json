{
  "expo": {
    "name": "Olis",
    "slug": "Olis",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "exp+olis",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.taka4655.olis",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-3940256099942544~1458002511",
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.taka4655.olis",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "com.google.android.gms.permission.AD_ID"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser",
      "expo-tracking-transparency"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "privacyPolicyUrl": "https://taku-enginner.github.io/apps/olis/index.html",
      "router": {},
      "eas": {
        "projectId": "29f5ef8b-8980-49f2-836d-1315d74169c1"
      }
    },
    "owner": "taka4655",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/af60afa3-cddc-400d-ac46-0de465205d00"
    }
  },
  "react-native-google-mobile-ads": {
    "ios_app_id": "ca-app-pub-6214815327107771~8358056419",
    "user_tracking_usage_description": "関連性の高い広告を表示するためにIDをトラッキングいたします。"
  }
}
